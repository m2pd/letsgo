extends layout/layout

block vars
    -title = 'Welcome to Gulp Template workflow'
    -bodyClass = 'home-page'


block container
    main.main-page.page-introduction
        .section-hero 
            .group-hero
                form
                    .input-group
                        input.form-control.form-control-custom(type='text' aria-label='Amount (to the nearest dollar)')
                        .icon-input
                            button.btn(type='submit')
                                span
                                    i.icon.fa.fa-search(aria-hidden='true')
                ul.category.category__list
                    - var list = ["Tour", "Dịch Vụ Lưu Trú","Xe du lịch","Booking guide","Hướng dẫn viên bản địa","Tour giờ chót","Tour thanh lý sang nhượn","Vé và combo du lịch"]
                        each item in list
                            li.category__item
                                +link('#') 
                                    | #{item}
        .group-section
            .section.section-group-banner.group-banner
                +img('../../images/hdvdl 1.jpg','banner 01')
            .section.section-user
                .container
                    .user-info.user
                        .user__avatar 
                            figure
                                +img('../../images/gallery_4.jpeg','avatar 01')
                        .user__data
                            p.text-title.user__name Trần Phạm Minh Tiến
                            .user__cotnact 
                                button.btn.btn-outline-primary.text-type-medium.mr-3(type='button')
                                    span.text-family-SemiBold CHAT NGAY 
                                button.btn.btn-outline-primary.text-type-medium(type='button') 
                                    span.text-family-SemiBold 1 đánh giá
                            .user__profile 
                                +link('#') 
                                    span.text-type-semibold.text-family-SemiBold.text-border-right Giới thiệu
                                +link('#') 
                                    span.text-type-semibold.text-family-SemiBold.text-border-right Ảnh
                                +link('#') 
                                    span.text-type-semibold.text-family-SemiBold.text-border-right Bài đánh giá
                                +link('#') 
                                    span.text-type-semibold.text-family-SemiBold Check in
                
            .section.section-profile 
                .container
                    .section-profile__inner
